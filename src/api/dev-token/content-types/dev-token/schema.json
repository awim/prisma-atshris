{
  "kind": "collectionType",
  "collectionName": "dev_tokens",
  "info": {
    "singularName": "dev-token",
    "pluralName": "dev-tokens",
    "displayName": "Development Token"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tokenName": {
      "type": "string",
      "required": true
    },
    "dueDate": {
      "type": "date"
    },
    "isActive": {
      "type": "boolean"
    },
    "initQuota": {
      "type": "integer",
      "max": 99999
    },
    "leftQuota": {
      "type": "integer",
      "min": 0,
      "max": 99999
    },
    "organization": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organization.organization",
      "inversedBy": "devTokens"
    },
    "tokenId": {
      "type": "uid",
      "targetField": ""
    }
  }
}
